<!DOCTYPE html>
<html>
	<head>
		{% block head %}
			<meta charset="UTF-8">
			<meta name="viewport" content="width=device-width, initial-scale=1.0">
			<title>
				{% block title %}{% endblock %}
			</title>
			<style>
				{% block stylesheets %}
					.example-wrapper {
						margin: 1em auto;
						max-width: 800px;
						width: 95%;
						font: 18px / 1.5 sans-serif;
					}
					.example-wrapper code {
						background: #F5F5F5;
						padding: 2px 6px;
					}
				{% endblock stylesheets %}
			</style>
		{% endblock head %}
	</head>
	<nav>
		{% block nav %}
			<ul>
				<li>
					<a href="{{path('app_home')}}">Home</a>
				</li>
				<li>
					<a href="{{path('app_pins_create')}}">Create</a>
				</li>
				{# <li><a href="{{path('app_pins_create')}}">Create</a></li> #}
			</ul>
		{% endblock nav %}
	</nav>
	<body>
		{% block body %}{% endblock body %}
	</body>
	{% block javascripts %}{% endblock javascripts %}
</html>
